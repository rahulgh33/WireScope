{
  "id": null,
  "uid": "queue",
  "title": "Queue",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Processing Duration (P95)",
      "datasource": "Prometheus",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum by (le) (rate(queue_processing_duration_seconds_bucket[5m])))", "legendFormat": "p95"}
      ]
    },
    {
      "type": "graph",
      "title": "Queue Lag (messages)",
      "datasource": "Prometheus",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [
        {"expr": "queue_lag_messages", "legendFormat": "lag"}
      ]
    },
    {
      "type": "graph",
      "title": "Ack Pending (messages)",
      "datasource": "Prometheus",
      "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
      "targets": [
        {"expr": "queue_ack_pending_messages", "legendFormat": "ack_pending"}
      ]
    },
    {
      "type": "graph",
      "title": "DLQ Messages",
      "datasource": "Prometheus",
      "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
      "targets": [
        {"expr": "rate(dlq_messages_total[5m])", "legendFormat": "dlq/s"}
      ]
    }
  ]
}
